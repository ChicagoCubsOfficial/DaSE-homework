   "source": [
    "import math\n",
    "a = float(input(\"请输入一个数: \"))\n",
    "def f(x):\n",
    "    return x**3 - a\n",
    "def F1(x):\n",
    "    return 3*x**2\n",
    "ep = 0.00000000001\n",
    "k = 1\n",
    "t = 1\n",
    "\n",
    "while True:\n",
    "    t = k\n",
    "    k = t - f(t) / F1(t)\n",
    "    print(t,k)\n",
    "    if math.fabs(k - t) < ep:\n",
    "        break\n",
    "print(k)"
   ]
