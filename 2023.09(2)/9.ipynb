   "source": [
    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    t = x * x + 4 * x * np.sin(x)\n",
    "    return t\n",
    "\n",
    "\n",
    "def Monte_Carlo(a, b):\n",
    "    # (a,b)是积分区间\n",
    "    x = np.random.uniform(a, b,1000000)\n",
    "    y = f(x)\n",
    "    sum = 0\n",
    "    for i in y:\n",
    "        sum += i\n",
    "\n",
    "    return sum / 1000000\n",
    "\n",
    "\n",
    "print(Monte_Carlo(2, 3))"
   ]
