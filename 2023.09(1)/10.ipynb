   "source": [
    "def root(n):\n",
    "    e = 1e-6\n",
    "    g = n/2.0\n",
    "    while abs(g ** 3 -n) > e:\n",
    "        g = (2 * g + n / g**2) / 3.0\n",
    "    return g\n",
    "n =float(input(\"请输入一个数：\"))\n",
    "r = root(n)\n",
    "print(f\"{n}的三次方根是约{r:.6f}\")"
   ]
