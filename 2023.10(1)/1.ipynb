    "n = int(input(\"请输入数字:\"))\n",
    "c = 0\n",
    "for i in range(2,n):\n",
    "    if(n % i == 0):\n",
    "        c = 1\n",
    "if(c == 1):\n",
    "    print(n,'不是质数')\n",
    "else:\n",
    "    print(n,'是质数')"
